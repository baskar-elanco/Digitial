{
    "parser": "babel-eslint",
    "extends": ["airbnb"],
    "env": {
      "browser": true,
      "node": true
    },
    "rules": {
      "react/jsx-filename-extension": "off",
      "react/jsx-wrap-multilines":"off",
      "max-len":"off",
      "no-mixed-spaces-and-tabs":"off",
      "no-tabs":"off",
      "import/extensions":"off",
      "react/destructuring-assignment":"off",
      "react/jsx-indent":"off"
    }
  }